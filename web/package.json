{
  "name": "opendaylight-spectrometer-web",
  "version": "0.0.1",
  "description": "OpenDaylight Spectrometer",
  "scripts": {
    "slate": "rm -rf node_modules && npm install",
    "clean": "rm -rf dist",
    "start": "set NODE_ENV=development && node src/server/index.js --progress --colors --profile",
    "start-prod": "set NODE_ENV=production && node src/server/index.js --progress --colors --profile",
    "build": "npm run clean && set NODE_ENV=production && webpack -p --progress --colors --profile",
    "test": "set NODE_ENV=test && mocha --compilers js:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://opendaylight.git"
  },
  "license": "EPL-1.0",
  "dependencies": {
    "axios": "0.7.0",
    "classnames": "^2.1.5",
    "express": "4.13.3",
    "history": "1.13.0",
    "lodash": "^4.5.1",
    "material-ui": "^0.14.4",
    "moment": "^2.11.2",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-shallow-compare": "^0.14.7",
    "react-dom": "^0.14.6",
    "react-input-autosize": "^0.6.8",
    "react-redux": "2.1.2",
    "react-router": "1.0.0-rc1",
    "react-selectize": "^0.8.1",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.0.5",
    "redux-router": "^1.0.0-beta7",
    "redux-thunk": "0.1.0",
    "redux-undo": "0.5.0",
    "victory": "^0.4.0",
    "victory-chart": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "css-loader": "0.9.0",
    "expect": "1.12.1",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.5",
    "jsdom-no-contextify": "3.1.0",
    "json-loader": "^0.5.4",
    "merge": "1.2.0",
    "mocha": "2.1.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "2.0.2",
    "style-loader": "0.8.0",
    "url-loader": "0.5.6",
    "webpack": "1.11.0",
    "webpack-dev-middleware": "1.2.0",
    "webpack-hot-middleware": "2.2.0"
  }
}
